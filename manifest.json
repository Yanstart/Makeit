{
    "manifest_version": 3,
    "name": "MakeItGreen!",
    "description" : "Voici une extension qui permet de v√©rifier la data score",
    "version": "1.0.1",

    "action": {
        "default_icon" : "iconA.png",
        "default_title": "MakeItGreen Now !",   
        "default_popup": "index.html" 
    },
    
    "web_accessible_resources": [
        {
            "resources": ["script.js", "style.css"],
            "matches": ["<all_urls>"]
        }
    ],

    "permissions": [
        "activeTab",
        "storage"
    ]

}  