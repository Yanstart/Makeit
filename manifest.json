{
    "manifest_version": 3,
    "name": "Make It Green",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Make It Green"
    }
  }